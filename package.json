{
  "name": "rasa-snippets",
  "displayName": "Rasa Snippets",
  "description": "A yaml snippets extension for rasa 3.x",
  "version": "0.0.1",
  "publisher": "uncle-lv",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "vsce": "^2.15.0"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "icon": "img/rasa-logo.png",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Rasa",
    "Chatbot",
    "yaml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uncle-lv/vscode-rasa-snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/common.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/config.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/credentials.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/domain.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/endpoints.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/nlu.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/responses.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/rules.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/slot.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/stories.code-snippets"
      }
    ]
  }
}
